#. module: shift
#: model:mail.template,body_html:shift.email_template_shift_summary
msgid ""
"\n"
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"\n"
"    <p>Hello <t t-out=\"object.worker_id.name or ''\"></t>,</p>\n"
"\n"
"    <p>You are awaited the <t t-out=\"format_datetime(object.start_time, tz=object.worker_id.tz or 'Europe/Brussels', dt_format='dd.MM.yyyy')\"></t>\n"
"    for the shift starting at <t t-out=\"format_datetime(object.start_time, tz=object.worker_id.tz or 'Europe/Brussels',dt_format='HH:mm')\"></t>.\n"
"\n"
"    <br><br>Please contact us at <t t-out=\"object.worker_id.company_id.email\"></t> if you have any trouble attending the shift.\n"
"    </p>\n"
"    <br>\n"
"    <p>Sustainably yours,</p>\n"
"    <p><t t-out=\"object.worker_id.company_id.name\"></t>.</p>\n"
"\n"
"    <t t-if=\"object.worker_id.company_id.street\">\n"
"        <t t-out=\"object.worker_id.company_id.street\"></t>\n"
"    </t>\n"
"    <t t-if=\"object.worker_id.company_id.street2\">\n"
"        <t t-out=\"object.worker_id.company_id.street2\"></t><br>\n"
"    </t>\n"
"    <t t-if=\"object.worker_id.company_id.city or object.worker_id.company_id.zip\">\n"
"        <t t-out=\"object.worker_id.company_id.zip\"></t> <t t-out=\"object.worker_id.company_id.city\"></t><br>\n"
"    </t>\n"
"    <t t-if=\"object.worker_id.company_id.country_id\">\n"
"        <t t-out=\"object.worker_id.company_id.state_id and ('%s, ' % object.worker_id.company_id.state_id.name) or ''\"></t> <t t-out=\"object.worker_id.company_id.country_id.name or ''\"></t><br>\n"
"    </t>\n"
"    <t t-if=\"object.worker_id.company_id.phone\">\n"
"        Phone:&nbsp; <t t-out=\"object.worker_id.company_id.phone\"></t>\n"
"    </t>\n"
"\n"
"    <t t-if=\"object.worker_id.company_id.website\">\n"
"        <div>\n"
"            Web :&nbsp;<a href=\"<t t-out=\" object.worker_id.company_id.website\"=\"\"></a>\"&gt;<t t-out=\"object.worker_id.company_id.website\"></t>\n"
"        </div>\n"
"    </t>\n"
"    <t t-if=\"object.worker_id.company_id.logo\">\n"
"    <div>\n"
"        <img src=\"<t\" t-out=\"object.worker_id.company_id.logo\">&gt;\n"
"    </div>\n"
"    </t>\n"
"</div>\n"
"            "
